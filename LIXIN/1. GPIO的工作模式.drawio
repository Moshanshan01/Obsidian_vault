        <mxCell id="4ACmVNNrFLLagpRrwYny-29" value="&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;普通IO&lt;/span&gt;&lt;div&gt;&lt;span style=&quot;text-wrap-mode: wrap;&quot;&gt;口输入&lt;/span&gt;&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;labelBorderColor=none;textShadow=0;labelPosition=center;verticalLabelPosition=middle;textDirection=vertical-lr;" vertex="1" parent="1">
